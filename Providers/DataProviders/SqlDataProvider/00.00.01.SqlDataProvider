GO
SET QUOTED_IDENTIFIER ON
GO
IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}{objectQualifier}[FM_MaggieDixonListings_GetRentListings]') AND type in (N'P', N'PC'))
BEGIN
EXEC dbo.sp_executesql @statement = N'

CREATE Procedure {databaseOwner}{objectQualifier}[FM_MaggieDixonListings_GetRentListings]

AS



BEGIN
	SELECT * FROM FM_MaggieDixonListings_Rent Where Status = ''Active'' or Status = ''New''   
	
END

' 
END
GO
/****** Object:  StoredProcedure {databaseOwner}{objectQualifier}[FM_MaggieDixonListings_GetBuyListings]    Script Date: 05/14/2015 17:42:40 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}{objectQualifier}[FM_MaggieDixonListings_GetBuyListings]') AND type in (N'P', N'PC'))
BEGIN
EXEC dbo.sp_executesql @statement = N'

CREATE Procedure {databaseOwner}{objectQualifier}[FM_MaggieDixonListings_GetBuyListings]

AS



BEGIN
	SELECT     dbo.FM_MaggieDixonListings_Buy.*
	FROM         dbo.FM_MaggieDixonListings_Buy
	WHERE     (Status = ''current'')
	UNION ALL
	SELECT     dbo.FM_MaggieDixonListings_Buy.*
	FROM         dbo.FM_MaggieDixonListings_Buy
	WHERE     (Status = ''sold'') AND ModifiedDate BETWEEN DATEADD(dd,-7,GETDATE()) AND GETDATE()   
		
END
' 
END
GO
/****** Object:  StoredProcedure {databaseOwner}{objectQualifier}[FM_MaggieDixonListings_AddRentListings]    Script Date: 05/14/2015 17:42:40 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}{objectQualifier}[FM_MaggieDixonListings_AddRentListings]') AND type in (N'P', N'PC'))
BEGIN
EXEC dbo.sp_executesql @statement = N'

CREATE Procedure {databaseOwner}{objectQualifier}[FM_MaggieDixonListings_AddRentListings]
@PropertyID nvarchar(50),
@Headline nvarchar(255),
@Description nvarchar(Max),
@Price money,
@StreetNo nvarchar(255),
@StreetName nvarchar(255),
@Suburb nvarchar(255),
@Category nvarchar(50),
@SubCategory nvarchar(50),
@Bedrooms int,
@Ensuites int,
@Bathrooms int,
@Garages int, 
@Features nvarchar(Max),
@Images nvarchar(Max),
@MainImage nvarchar(50),
@DateAvailable nvarchar(255),
@ModifiedDate datetime,
@Status nvarchar(50)


AS


IF EXISTS (SELECT * FROM FM_MaggieDixonListings_Rent WHERE PropertyID = @PropertyID)
BEGIN
    --UPDATE HERE
    UPDATE FM_MaggieDixonListings_Rent
    SET
		Headline = @Headline,
		[Description] = @Description,
		Price = @Price,
		StreetNo = @StreetNo,
		StreetName = @StreetName,
		Suburb = @Suburb,
		Category = @Category,
		SubCategory = @SubCategory,
		Bedrooms = @Bedrooms,
		Ensuites = @Ensuites,
		Bathrooms = @Bathrooms,
		Garages = @Garages, 
		Features = @Features,		
		Images = @Images,
		MainImage = @MainImage,
		DateAvailable = @DateAvailable,
		ModifiedDate = @ModifiedDate,
		[Status] = @Status		
	WHERE PropertyID = @PropertyID
END
ELSE
BEGIN
   -- INSERT HERE
	INSERT INTO FM_MaggieDixonListings_Rent
		(PropertyID,
		Headline,
		[Description],
		Price,
		StreetNo,
		StreetName,
		Suburb,
		Category,
		SubCategory,
		Bedrooms,
		Ensuites,
		Bathrooms,
		Garages, 
		Features,
		Images,
		MainImage,
		DateAvailable,
		ModifiedDate,
		[Status])
	VALUES
		(@PropertyID,
		@Headline,
		@Description,
		@Price,
		@StreetNo,
		@StreetName,
		@Suburb,
		@Category,
		@SubCategory,
		@Bedrooms,
		@Ensuites,
		@Bathrooms,
		@Garages, 
		@Features,		
		@Images,
		@MainImage,
		@DateAvailable,
		@ModifiedDate,
		@Status)
	
END

--------------------




' 
END
GO
