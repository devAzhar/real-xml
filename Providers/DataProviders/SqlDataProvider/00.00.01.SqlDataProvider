		Images,
		MainImage,
		ModifiedDate,
		[Status])
	VALUES
		(@PropertyID,
		@Headline,
		@Description,
		@Price,
		@StreetNo,
		@StreetName,
		@Suburb,
		@Category,
		@SubCategory,
		@Bedrooms,
