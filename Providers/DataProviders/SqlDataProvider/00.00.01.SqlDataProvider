CREATE Procedure {databaseOwner}{objectQualifier}[FM_MaggieDixonListings_AddBuyListings]
@PropertyID nvarchar(50),
@Headline nvarchar(255),
@Description nvarchar(Max),
@Price money,
@StreetNo nvarchar(255),
@StreetName nvarchar(255),
@Suburb nvarchar(255),
@Category nvarchar(50),
@SubCategory nvarchar(50),
@Bedrooms int,
@Ensuites int,
@Bathrooms int,
@Garages int, 
