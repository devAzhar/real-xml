GO
IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}{objectQualifier}[FM_EProfile_GetUserLogin]') AND type in (N'P', N'PC'))
BEGIN
EXEC dbo.sp_executesql @statement = N'CREATE Procedure {databaseOwner}{objectQualifier}[FM_EProfile_GetUserLogin]      
@UserName nvarchar(100),    
@PortalID int    
       
AS      
SELECT     userid as xid ,0 as ProfileID 
FROM       users' 
END
